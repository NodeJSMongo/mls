        <input type="checkbox" name="area" value="Ramna">Ramna<br>
        <input type="checkbox" name="area" value="Rampura">Rampura<br>
        <input type="checkbox" name="area" value="Shabujbagh">Shabujbagh<br>
        <input type="checkbox" name="area" value="Shah Ali">Shah Ali<br>
