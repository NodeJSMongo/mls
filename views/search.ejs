<% include partials/header %>
<% include partials/loggedin %>
<div class="container bottomPadding">

  <form class="" action="/listing" method="GET">
  <div class="row bottomPadding">
    <fieldset class="col">
        <legend>Class</legend>
        <input type="checkbox" name="propertyclass" value="Residential">Residential<br>
        <input type="checkbox" name="propertyclass" value="Commercial">Commercial<br>
    </fieldset>
    <fieldset class="col">
        <legend>Availability</legend>
        <input type="checkbox" name="status" value="Available">Available<br>
        <input type="checkbox" name="status" value="Unvailable">Unvailable<br>
    </fieldset>
    <fieldset class="col">
        <legend>Sale or Lease</legend>
        <input type="checkbox" name="sale" value="Sale">Sale<br>
        <input type="checkbox" name="sale" value="Lease">Lease<br>
        <input type="checkbox" name="sale" value="Sub Lease">Sub-Lease<br>
    </fieldset>
  </div>
  <hr>
